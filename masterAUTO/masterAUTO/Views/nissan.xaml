<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="masterAUTO.Views.nissan">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" Margin="10">
            <Image x:Name="kartinka"></Image>
            <Label x:Name="header" Text="Модели Nissan:" FontSize="Medium" />
            <Picker x:Name="picker" SelectedIndexChanged="picker_SelectedIndexChanged">
                <Picker.Items>
                    <x:String>Nissan GT-R R35 2018</x:String>
                    <x:String>Nissan Skyline GT-R R34</x:String>
                    <x:String>Nissan Laurel C35</x:String>
                    <x:String>Nissan Silvia S15</x:String>
                </Picker.Items>
            </Picker>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>